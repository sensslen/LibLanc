---
Language: Cpp
BasedOnStyle: Google
ColumnLimit: 120
BreakBeforeBraces: Custom
AccessModifierOffset: -2
BraceWrapping:
  AfterCaseLabel: true
  AfterClass: true
  AfterControlStatement: Always
  AfterEnum: true
  AfterFunction: true
  AfterNamespace: true
  AfterObjCDeclaration: true
  AfterStruct: true
  AfterUnion: true
  AfterExternBlock: true
  BeforeCatch: true
  BeforeElse: true
  BeforeLambdaBody: true
  BeforeWhile: true
  IndentBraces: false
  SplitEmptyFunction: true
  SplitEmptyRecord: true
  SplitEmptyNamespace: true
AllowShortFunctionsOnASingleLine: Inline
AllowShortIfStatementsOnASingleLine: Never
BinPackArguments: false
BinPackParameters: false
IndentWidth: 4
AlignAfterOpenBracket: AlwaysBreak
BreakConstructorInitializers: BeforeComma
Cpp11BracedListStyle: false
SpaceAfterTemplateKeyword: false
AllowShortLoopsOnASingleLine: false
DerivePointerAlignment: false
PointerAlignment: Right
MacroBlockBegin: PROAPI_ENUM_BEGIN
MacroBlockEnd: PROAPI_ENUM_END
AttributeMacros: [ '__capability', 'STDMETHODCALLTYPE', 'CALLBACK' ]
WhitespaceSensitiveMacros: [ 'STRINGIZE', 'PRO_DISABLE_WARNING' ]
StatementMacros: [ 'rmt_ScopedCPUSample' ]
